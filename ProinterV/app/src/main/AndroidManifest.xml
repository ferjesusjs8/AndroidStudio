<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anhanguera.prointerv">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ACTIVITY.FormularioPublicacaoActivity"></activity>
        <activity android:name=".ACTIVITY.FormularioGrupoActivity" />
        <activity
            android:name=".ACTIVITY.PublicacaoActivity"
            android:label="@string/title_activity_publicacoes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ACTIVITY.GrupoActivity"
            android:label="@string/title_activity_grupos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ACTIVITY.MainActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ACTIVITY.FormularioAlunoActivity" />
        <activity android:name=".ACTIVITY.AlunoActivity" />
    </application>

</manifest>